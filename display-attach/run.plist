<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN http://www.apple.com/DTDs/PropertyList-1.0.dtd >
<plist version="1.0">
  <dict>

    <key>Label</key>
    <string>com.romeo.display-attach</string>

    <key>ProgramArguments</key>
    <array>
      <string>sh</string>
      <string>/Users/romeo/.config/display-attach/run.sh</string>
    </array>

    <key>StartInterval</key>
    <integer>2</integer>

    <key>ThrottleInterval</key>
    <integer>1</integer>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <false/>
  </dict>
</plist>
