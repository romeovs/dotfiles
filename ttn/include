
TTNROOT=$HOME/code/go/src/github.com/TheThingsNetwork

alias ttnctll="ttnctl --config $HOME/.ttnctl.local.yaml"
alias ttnctls="ttnctl --config $HOME/.ttnctl.staging.yaml"
alias ttnctld="$TTNROOT/ttn/release/ttnctl-darwin-amd64 --config $HOME/.ttnctl.local.yaml"
alias ttnctlr="$TTNROOT/ttn/release/ttnctl-darwin-amd64 --config $HOME/.ttnctl.refactor.yaml"

